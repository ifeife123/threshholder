<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Risk Tolerance (epsilon)"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:text="Air Exchange Per Hour ACH"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Qb Breathing rate"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mask Filtration"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Data To Get Safety Advice"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:text="Number of People in Room:"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextPt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editTextTimeExposure"
        app:layout_constraintEnd_toEndOf="@+id/editTextTimeExposure"
        app:layout_constraintTop_toBottomOf="@+id/editTextNpeople" />

    <EditText
        android:id="@+id/editTextTimeExposure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editTextV"
        app:layout_constraintEnd_toEndOf="@+id/editTextV"
        app:layout_constraintTop_toBottomOf="@+id/editTextPt" />

    <EditText
        android:id="@+id/editTextV"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editTextCo"
        app:layout_constraintEnd_toEndOf="@+id/editTextCo"
        app:layout_constraintTop_toBottomOf="@+id/editTextTimeExposure" />

    <EditText
        android:id="@+id/editTextCo"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editTextRiskTolerance"
        app:layout_constraintEnd_toEndOf="@+id/editTextRiskTolerance"
        app:layout_constraintTop_toBottomOf="@+id/editTextV" />

    <EditText
        android:id="@+id/editTextRiskTolerance"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        android:text="@string/epsilon_default"
        app:layout_constraintBottom_toTopOf="@+id/editTextACH"
        app:layout_constraintEnd_toEndOf="@+id/editTextACH"
        app:layout_constraintTop_toBottomOf="@+id/editTextCo" />

    <EditText
        android:id="@+id/editTextACH"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        android:text="@string/ACH_default"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/editTextQb"
        app:layout_constraintTop_toBottomOf="@+id/editTextRiskTolerance" />

    <EditText
        android:id="@+id/editTextQb"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        android:text="@string/Qb_default"
        app:layout_constraintBottom_toTopOf="@+id/editTextPm"
        app:layout_constraintEnd_toEndOf="@+id/editTextPm"
        app:layout_constraintTop_toBottomOf="@+id/editTextACH" />

    <EditText
        android:id="@+id/editTextCq"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editTextSr"
        app:layout_constraintEnd_toEndOf="@+id/editTextSr"
        app:layout_constraintTop_toBottomOf="@+id/editTextPm" />

    <EditText
        android:id="@+id/editTextSr"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/editTextCq" />

    <EditText
        android:id="@+id/editTextPm"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editTextCq"
        app:layout_constraintEnd_toEndOf="@+id/editTextCq"
        app:layout_constraintTop_toBottomOf="@+id/editTextQb" />

    <EditText
        android:id="@+id/editTextNpeople"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/data_hint_text"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/editTextPt"
        app:layout_constraintEnd_toEndOf="@+id/editTextPt"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Production Rate:"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Exposure Time:"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Threshold (or Co):"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Volume:"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textViewOutput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:layout_editor_absoluteX="13dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="displayTMax"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSr" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="138dp"
        android:layout_height="43dp"
        android:text="Cq - concentration of infection quanta"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="117dp"
        android:layout_height="37dp"
        android:text="Sr - relative transmissability"
        app:layout_constraintBottom_toTopOf="@+id/textViewOutput"
        app:layout_constraintStart_toStartOf="@+id/textViewOutput"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />


</androidx.constraintlayout.widget.ConstraintLayout>